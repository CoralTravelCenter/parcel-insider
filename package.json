{
    "name": "parcel-insider",
    "version": "1.0.0",
    "description": "Insider mods powered by parceljs",
    "scripts": {
        "build-order-comments-field-coral": "parcel build --no-source-maps --dist-dir order-comments-field/coral/dist order-comments-field/coral/src/insider.js",
        "build-emergency-contact-field-coral": "parcel build --no-source-maps --dist-dir emergency-contact-field/coral/dist emergency-contact-field/coral/src/insider.js",
        "build-otium-hotels-search-coral": "parcel build --no-source-maps --dist-dir otium-hotel/coral/dist otium-hotel/coral/src/search-results.js",
        "build-otium-hotel-page-coral": "parcel build --no-source-maps --dist-dir otium-hotel/coral/dist otium-hotel/coral/src/hotel-page.js",
        "build-sfc-info-tab-coral": "parcel build --no-source-maps --dist-dir sfc-info-tab/coral/dist sfc-info-tab/coral/src/insider.js",
        "build-sfc-info-tab-sunmar": "parcel build --no-source-maps --dist-dir sfc-info-tab/sunmar/dist sfc-info-tab/sunmar/src/insider.js",
        "build-apply-form-where2buy-coral": "parcel build --no-source-maps --no-optimize --dist-dir apply-form-where-to-buy/coral/dist apply-form-where-to-buy/coral/src/insider.js",
        "build-friendly-404-v3-coral": "parcel build --no-source-maps --dist-dir friendly-404-v3/coral/dist friendly-404-v3/coral/src/index.js",
        "build-friendly-404-v3-sunmar": "parcel build --no-source-maps --dist-dir friendly-404-v3/sunmar/dist friendly-404-v3/sunmar/src/index.js",
        "build-elite-service-tooltip-coral": "parcel build --no-source-maps --dist-dir elite-service-tooltip/coral/dist elite-service-tooltip/coral/src/index.js",
        "build-cyberweekend-video-banner-coral": "parcel build --no-source-maps --dist-dir cyberweekend-video-banner/coral/dist cyberweekend-video-banner/coral/src/index.js",
        "build-cyberweekend-discount-badge-coral": "parcel build --no-source-maps --dist-dir cyberweekend-discount-badge/coral/dist cyberweekend-discount-badge/coral/src/index.js",
        "build-cyberweekend-discount-badge-sunmar": "parcel build --no-source-maps --dist-dir cyberweekend-discount-badge/sunmar/dist cyberweekend-discount-badge/sunmar/src/index.js",
        "build-double-discount-badge-coral": "parcel build --no-source-maps --dist-dir double-discount-badge/coral/dist double-discount-badge/coral/src/index.js"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@parcel/optimizer-data-url": "^2.8.2",
        "@parcel/transformer-inline-string": "^2.8.2",
        "@parcel/transformer-less": "^2.8.2",
        "@parcel/transformer-yaml": "^2.8.2",
        "mustache": "^4.2.0",
        "parcel": "^2.8.2",
        "parcel-resolver-ignore": "^2.1.3"
    },
    "parcelIgnore": [
        "{{.*}}"
    ]
}
